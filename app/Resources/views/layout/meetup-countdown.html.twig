<div class="meetup-countdown">
    <div class="row">
        <div class="col-lg-12 center">
            <h2>{{ meetup.name }}</h2>
            <hr>
            <h3>Starts in 05 days, 07 hours, 42 minutes, 36 seconds</h3>
            <p>{{ meetup.yes_rsvp_count }} coders already signed up!</p>
            <p>
                <a class="btn btn-success btn-lg" href="{{ meetup.event_url }}">Attend our Meetup!</a>
            </p>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <iframe frameborder="0" scrolling="no" marginheight="0" marginwidth="0" width="100%" height="200"
                src="https://maps.google.com/maps?hl=en&q={{ meetup.venue.address_1 }}&ie=UTF8&t=roadmap&z=12&iwloc=B&output=embed">
        </iframe>
    </div>
</div>
<div class="row">
    <div class="col-lg-12" class="meetupMasonry">
        <ul class="meetupMasonryPictures">
            {% for attendee in rsvpList %}
                {% if attendee.member_photo is defined %}
                    <li><img src="{{ attendee.member_photo.photo_link }}" title="{{ attendee.member.name }}" class="img-responsive"></li>
                {% endif %}
            {% endfor %}
        </ul>
    </div>
</div>
